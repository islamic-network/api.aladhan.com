{
    "name": "mamluk/kipchak-template",
    "type": "api",
    "description": "AlAdhan.com API",
    "authors": [
        {
            "name": "Meezaan-ud-Din Abdu Dhil-Jalali Wal-Ikram",
            "email": "support@islamic.network"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/meezaan/php-haversine-formula"
        }
    ],
    "require": {
        "ext-calendar": "*",
        "ext-gd": "*",
        "7x/sdk": "^1.7",
        "islamic-network/alqibla": "^1.2",
        "islamic-network/calendar": "^1.8",
        "islamic-network/prayer-times": "^1.0.29",
        "mamluk/kipchak": "^0.44",
        "p3k/emoji-detector": "^0.2.1",
        "rafaelfragoso/haversini-formula": "dev-master",
        "zircote/swagger-php": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "api/",
            "Database\\": "database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
